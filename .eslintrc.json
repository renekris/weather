{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-use-before-define": "off",
    "no-console": "off",
    "no-undef": "off",
    "no-unused-expressions": ["error", { "allowTernary": true }]
  }
}
